<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".TripRequest2" android:id="@+id/trip_request2" android:minWidth="1480dp"
        android:maxWidth="1480dp" android:minHeight="2040dp" android:maxHeight="2040dp">

    <TextView
            android:text="@string/trip_request"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/trip_request_text" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.0"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/black" android:textAlignment="center"/>
    <TextView
            android:text="@string/mumbai_central"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/mumbai_central"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/black" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/trip_request_text" app:layout_constraintVertical_bias="0.0"
            android:textAlignment="center"/>
    <Button
            android:text="@string/eighteen"
            android:layout_width="28dp"
            android:layout_height="20dp"
            app:layout_constraintStart_toStartOf="@+id/chats" app:layout_constraintEnd_toEndOf="@+id/chats"
            android:id="@+id/eighteen" app:layout_constraintHorizontal_bias="0.28"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/mumbai_central"
            android:textAppearance="@android:style/TextAppearance" android:textSize="12sp"
            android:textColor="@android:color/white" android:background="@android:color/holo_blue_dark"
            app:layout_constraintVertical_bias="0.0"/>
    <Button
            android:text="@string/aug"
            android:layout_width="28dp"
            android:layout_height="20dp"
            android:id="@+id/aug" app:layout_constraintStart_toEndOf="@+id/eighteen"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/mumbai_central"
            app:layout_constraintVertical_bias="0.0" android:background="@android:color/holo_blue_dark"
            android:textAppearance="@android:style/TextAppearance.DeviceDefault" android:textSize="12sp"
            android:textColor="@android:color/white"/>
    <Button
            android:text="@string/trip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:id="@+id/button3"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/eighteen"
            app:layout_constraintVertical_bias="1.0" android:textAppearance="@android:style/TextAppearance"
            android:textSize="12sp" android:textColor="@android:color/holo_blue_dark"/>
    <Button
            android:text="@string/chats"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/chats"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/eighteen"
            app:layout_constraintVertical_bias="1.0" android:textAppearance="@android:style/TextAppearance"
            android:textSize="12sp" android:textColor="@android:color/black"/>
    <Button
            android:text="@string/profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/profile" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/eighteen" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="1.0"
            android:textAppearance="@android:style/TextAppearance" android:textSize="12sp"
            android:textColor="@android:color/black"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="?android:attr/actionModeCloseDrawable"
            app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/back_button"
            app:layout_constraintEnd_toStartOf="@+id/trip_request_text" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/button3"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"
            android:tint="@android:color/holo_blue_dark" android:background="@android:color/transparent"
            android:onClick="goToTripRequestScreenFromTripRequest2ScreenViaBackButton" android:visibility="visible"/>
    <TextView
            android:text="@string/mumbai_central"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:id="@+id/mumbai_central_2" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toEndOf="@+id/eighteen"
            app:layout_constraintTop_toBottomOf="@+id/eighteen"
            android:textAppearance="@android:style/TextAppearance" android:textSize="12sp"
            android:textColor="@android:color/black"/>
    <TextView
            android:text="@string/exit_3"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:id="@+id/exit_3" app:layout_constraintStart_toStartOf="@+id/mumbai_central_2"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toEndOf="@id/mumbai_central_2"
            app:layout_constraintTop_toBottomOf="@+id/mumbai_central_2"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/black" android:textAlignment="center"
            app:layout_constraintBottom_toTopOf="@+id/button3" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="@string/mumbai_central"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:id="@+id/mumbai_central_3" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="1.0" app:layout_constraintEnd_toEndOf="@+id/platform_b"
            app:layout_constraintTop_toTopOf="@+id/mumbai_central_2"
            android:textAppearance="@android:style/TextAppearance"
            android:textSize="12sp" android:textColor="@android:color/black" tools:text="@string/mumbai_central"/>
    <TextView
            android:text="TextView"
            android:layout_width="87dp"
            android:layout_height="wrap_content"
            android:id="@+id/platform_b"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/mumbai_central_3"
            android:textAppearance="@android:style/TextAppearance"
            android:textSize="14sp" android:textColor="@android:color/black" tools:text="@string/platform_b"
            android:textAlignment="center"/>
    <TextView
            android:text="@string/two_luggages"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/two_luggages" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toStartOf="@+id/exit_3"
            app:layout_constraintTop_toBottomOf="@+id/exit_3" app:layout_constraintBottom_toTopOf="@+id/twenty_eight"
            app:layout_constraintVertical_bias="0.0" android:textAppearance="@android:style/TextAppearance"
            android:textSize="14sp" android:textColor="@android:color/black"/>
    <TextView
            android:text="@string/twenty_eight"
            android:layout_width="wrap_content"
            android:layout_height="16dp"
            android:id="@+id/twenty_eight" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toStartOf="@+id/total"
            app:layout_constraintTop_toBottomOf="@+id/two_luggages"
            android:textSize="12sp" android:textColor="@android:color/darker_gray"/>
    <TextView
            android:text="@string/kg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/kg" app:layout_constraintStart_toEndOf="@+id/twenty_eight"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toStartOf="@+id/platform_b"
            app:layout_constraintTop_toBottomOf="@+id/two_luggages"
            app:layout_constraintBottom_toTopOf="@+id/total" app:layout_constraintVertical_bias="0.0"
            android:textAppearance="@android:style/TextAppearance" android:textSize="12sp"
            android:textColor="@android:color/darker_gray" android:layout_marginStart="4dp"/>
    <TextView
            android:text="@string/total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/total" app:layout_constraintStart_toEndOf="@+id/kg"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/two_luggages"
            app:layout_constraintVertical_bias="0.0" android:textAppearance="@android:style/TextAppearance"
            android:textSize="12sp" android:textColor="@android:color/darker_gray" android:layout_marginStart="4dp"/>
    <Button
            android:text="@string/dollar_sign"
            android:layout_width="16dp"
            android:layout_height="33dp"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/dollar_sign_1"
            app:layout_constraintTop_toBottomOf="@+id/platform_b"
            app:layout_constraintStart_toStartOf="@+id/mumbai_central_3" app:layout_constraintHorizontal_bias="0.041"
            app:layout_constraintEnd_toStartOf="@+id/platform_b"
            android:background="@android:color/holo_blue_dark" android:textSize="14sp"
            android:textColor="@android:color/white" android:textAlignment="viewEnd"/>
    <Button
            android:text="@string/ten"
            android:layout_width="23dp"
            android:layout_height="33dp"
            app:layout_constraintTop_toBottomOf="@+id/platform_b" app:layout_constraintBottom_toTopOf="@+id/dash"
            app:layout_constraintStart_toEndOf="@+id/dollar_sign_1" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent" android:background="@android:color/holo_blue_dark"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/white" app:layout_constraintVertical_bias="0.0"
            android:textAlignment="viewStart" android:id="@+id/ten"/>
    <Button
            android:text="@string/dash"
            android:layout_width="12dp"
            android:layout_height="33dp"
            android:id="@+id/dash"
            app:layout_constraintTop_toBottomOf="@+id/platform_b"
            app:layout_constraintStart_toEndOf="@+id/ten" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintEnd_toEndOf="parent" android:background="@android:color/holo_blue_dark"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/white" app:layout_constraintVertical_bias="0.0"
            android:textAlignment="center"
            app:layout_constraintBottom_toTopOf="@+id/profile"/>
    <Button
            android:text="@string/dollar_sign"
            android:layout_width="13dp"
            android:layout_height="33dp"
            android:id="@+id/dollar_sign_2" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/platform_b"
            app:layout_constraintStart_toEndOf="@+id/dash"
            android:background="@android:color/holo_blue_dark" android:textAppearance="@android:style/TextAppearance"
            android:textSize="14sp" android:textColor="@android:color/white"
            app:layout_constraintHorizontal_bias="0.0" android:textAlignment="viewEnd"/>
    <Button
            android:text="@string/twenty"
            android:layout_width="0dp"
            android:layout_height="33dp"
            android:id="@+id/twenty"
            android:background="@android:color/holo_blue_dark"
            android:textAppearance="@android:style/TextAppearance" android:textSize="14sp"
            android:textColor="@android:color/white"
            app:layout_constraintStart_toEndOf="@+id/dollar_sign_2" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/platform_b"
            app:layout_constraintBottom_toTopOf="@+id/profile" app:layout_constraintVertical_bias="0.0"
            android:textAlignment="viewStart"/>
    <Button
            android:text="@string/confirm"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/chats" app:layout_constraintEnd_toEndOf="@+id/chats"
            android:id="@+id/confirm"
            app:layout_constraintTop_toBottomOf="@+id/twenty_eight" app:layout_constraintBottom_toTopOf="@+id/chats"
            android:background="@android:color/holo_blue_dark" android:textAppearance="@android:style/TextAppearance"
            android:textSize="14sp" android:textColor="@android:color/white"
            app:layout_constraintVertical_bias="0.902" style="@style/Widget.AppCompat.Button.Borderless"
            android:onClick="goToTripRequestBiddingScreenFromTripRequest2ScreenViaConfirmButton"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@android:drawable/ic_menu_send"
            android:id="@+id/another_right_arrow"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/eighteen"
            app:layout_constraintBottom_toTopOf="@+id/confirm" app:layout_constraintVertical_bias="0.0"
            android:background="@android:color/transparent" android:tint="@android:color/holo_blue_dark"
            android:layout_marginTop="3dp"/>
</android.support.constraint.ConstraintLayout>